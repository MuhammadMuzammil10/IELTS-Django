{
    "builds": [
        {
            "src": "backend/ielts_app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static-"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/ielts_app/wsgi.py"
        }
    ]
}